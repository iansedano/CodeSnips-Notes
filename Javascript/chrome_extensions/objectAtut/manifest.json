{
    "name": "obj ext",
    "description": "my extensione",
    "version": "1",
    "manifest_version": 2,
    "background": {
        "scripts":["background.js"]
    },
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.google.com/*",
        "https://stackoverflow.com/*",
        "storage"
    ]
}